        <attr name="gif" format="reference" />
        <attr name="paused" format="boolean" />
    </declare-styleable>

    <style name="Widget.GifMoviewView" parent="@android:style/Widget" />


    <declare-styleable name="SettingLayout">
        <attr name="setting_name" format="string" />
